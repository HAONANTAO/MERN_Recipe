# Ignore node_modules directory
node_modules/

# Ignore local development environment configuration files
.env.local
.env.development.local
.env.test.local
.env.production.local

# Ignore build output directories
build/
dist/

# Ignore editor configuration files
.vscode/
.idea/

# Ignore log files
*.log
*.log.*
npm-debug.log*

# Ignore temporary files
*.tmp
*.swp

# Ignore dependency files
package-lock.json
yarn.lock

# Ignore generated static files
*.html
*.css
*.js

# Ignore backup files
*.bak
*.backup

# Ignore system files
.DS_Store
Thumbs.db
